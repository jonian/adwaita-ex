[Icon Theme]
Name=AdwaitaEx
Comment=A simple and modern icon theme based on Adwaita.
Inherits=Adwaita

# Directory list
Directories=scalable/apps,symbolic/apps

# Applications

[scalable/apps]
Context=Applications
Size=128
MinSize=8
MaxSize=512
Type=Scalable

[symbolic/apps]
Context=Applications
Size=16
MinSize=8
MaxSize=512
Type=Scalable
