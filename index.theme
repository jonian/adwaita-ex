[Icon Theme]
Name=AdwaitaEx
Comment=A simple and modern icon theme based on Adwaita.
Inherits=Adwaita

# Directory list
Directories=apps/scalable,apps/symbolic

# Applications

[apps/scalable]
Context=Applications
Size=128
MinSize=8
MaxSize=512
Type=Scalable

[apps/symbolic]
Context=Applications
Size=16
MinSize=8
MaxSize=512
Type=Scalable
